{
  "type" :"module",
  "name": "fullstack-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"nodemon server.js\" \"npm run dev --prefix frontend\"",
    "build": "npm run build --prefix frontend",
    "start": "NODE_ENV=production node server.js",
    "start:frontend": "npm run start --prefix frontend",
    "start:fullstack": "concurrently \"npm run start\" \"npm run start:frontend\"",
     "install:all": "npm install --force && npm install --prefix backend --force && npm install --prefix frontend --force"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.0.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "concurrently": "^8.0.0"
  }
}
